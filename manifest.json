{
    "name": "微信公众号抓取器",
    "manifest_version": 2,
    "version": "1.0",
    "description": "微信公众号抓取器",
    "icons" : {
      "16" : "icon.png",
      "48" : "icon.png",
      "128" : "icon.png"
    },
    "browser_action": {
      "default_icon": "icon.png"
    },
    "content_scripts": [{
        "matches": ["https://mp.weixin.qq.com/mp/profile_ext?*"],
        "js": [
            "js/main.js"       
        ]
      }]
  }